{
  "ReverseProxy": {
    "Routes": {
      "runsRoute": {
        "ClusterId": "orch",
        "Match": { "Path": "/runs/{**catch-all}" },
        "Transforms": [ { "PathPattern": "/runs/{**catch-all}" } ]
      },
      "resultsRoute": {
        "ClusterId": "orch",
        "Match": { "Path": "/results/{**catch-all}" },
        "Transforms": [ { "PathPattern": "/results/{**catch-all}" } ]
      }
    },
    "Clusters": {
      "orch": {
        "Destinations": {
          "d1": { "Address": "http://orchestrator:8080" }
        }
      }
    }
  }
}