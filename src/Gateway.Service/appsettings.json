{
  "ReverseProxy": {
    "Routes": {
      "orch": {
        "ClusterId": "orch",
        "Match": { "Path": "/api/{**catch-all}" },
        "Transforms": [ { "PathPattern": "/{**catch-all}" } ]
      }
    },
    "Clusters": {
      "orch": {
        "Destinations": { "d1": { "Address": "http://orchestrator:8080" } }
      }
    }
  }
}